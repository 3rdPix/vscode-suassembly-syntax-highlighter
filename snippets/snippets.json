{
    "mov": {
        "prefix": "MOV",
        "body": "MOV ${1:A}, ${2:B}",
        "description": "Mover un valor desde un registro, hacia un registro, o desde una dirección de memoria hacia un registro.",
        "scope": "source.suasm"
    },
    "add": {
        "prefix": "ADD",
        "body": "ADD ${1:A}, ${2:B}",
        "description": "Sumar los valores de dos registros, o un registro y un literal.",
        "scope": "source.suasm"
    },
    "sub": {
        "prefix": "SUB",
        "body": "SUB ${1:A}, ${2:B}",
        "description": "Restar los valores de dos registros, o el registro A y un literal",
        "scope": "source.suasm"
    },
    "and": {
        "prefix": "AND",
        "body": "AND ${1:A}, ${2:B}",
        "description": "Calcular un AND lógico",
        "scope": "source.suasm"
    },
    "or": {
        "prefix": "OR",
        "body": "OR ${1:A}, ${2:B}",
        "description": "Calcular un OR lógico",
        "scope": "source.suasm"
    },
    "xor": {
        "prefix": "XOR",
        "body": "XOR ${1:A}, ${2:B}",
        "description": "Caclular un XOR lógico",
        "scope": "source.suasm"
    },
    "not": {
        "prefix": "NOT",
        "body": "NOT ${1:A}",
        "description": "Complemento a uno.",
        "scope": "source.suasm"
    },
    "shl": {
        "prefix": "SHL",
        "body": "SHL ${1:A}",
        "description": "Multiplicar por 2",
        "scope": "source.suasm"
    },
    "shr": {
        "prefix": "SHR",
        "body": "SHR ${1:A}",
        "description": "Dividir por 2",
        "scope": "source.suasm"
    },
    "inc": {
        "prefix": "INC",
        "body": "INC ${1:A}",
        "description": "Suma 1 al registro indicado",
        "scope": "source.suasm"
    },
    "dec": {
        "prefix": "DEC",
        "body": "DEC A",
        "description": "Resta 1 al registro A",
        "scope": "source.suasm"
    },
    "cmp": {
        "prefix": "CMP",
        "body": "CMP ${1:A}, ${2:B}",
        "description": "Compara dos valores (aka los resta) actualizando solo el registro Status",
        "scope": "source.suasm"
    },
    "JMP": {
        "prefix": "JMP",
        "body": "JMP ${1:label}",
        "description": "Salta incondicionalmente a la dirección asociada al label",
        "scope": "source.suasm"
    },
    "JEQ": {
        "prefix": "JEQ",
        "body": "JEQ ${1:label}",
        "description": "Salta al label si se cumple z = 1 (si la última instrucción es una comparación, esto se cumple si son iguales)",
        "scope": "source.suasm"
    },
    "JNE": {
        "prefix": "JNE",
        "body": "JNE ${1:label}",
        "description": "Salta al label si se cumple z = 0 (si la última instrucción es una comparación, esto se cumple si no son iguales)",
        "scope": "source.suasm"
    },
    "nop": {
        "prefix": "NOP",
        "body": "NOP",
        "description": "Instrucción placeholder, no hace nada",
        "scope": "source.suasm"
    },
    "jgt": {
        "prefix": "JGT",
        "body": "JGT ${1:label}",
        "description": "Salta al label si se cumple n=0 y z=0",
        "scope": "source.suasm"
    },
    "jge": {
        "prefix": "JGE",
        "body": "JGE ${1:label}",
        "description": "Salta al label si se cumple n=0",
        "scope": "source.suasm"
    },
    "jlt": {
        "prefix": "JLT",
        "body": "JLT ${1:label}",
        "description": "Salta al label si se cumple n=1",
        "scope": "source.suasm"
    },
    "jle": {
        "prefix": "JLE",
        "body": "JLE ${1:label}",
        "description": "Salta al label si se cumple n=1 ó z=1",
        "scope": "source.suasm"
    },
    "jcr": {
        "prefix": "JCR",
        "body": "JCR ${1:label}",
        "description": "Salta al label si se cumple c=1",
        "scope": "source.suasm"
    }
}