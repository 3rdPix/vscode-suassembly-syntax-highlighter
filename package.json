{
    "name": "suasm",
    "displayName": "Su-Assembly Syntax Highlighter",
    "description": "Extensi√≥n para habilitar syntax highlighting en arqui :D",
    "version": "0.2.1",
    "publisher": "3rdPix",
    "engines": {
      "vscode": "^1.60.0"
    },
    "icon": "images/su.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/3rdPix/vscode-suassembly-syntax-highlighter"
    },
    "bugs": {
        "type": "git",
        "url": "https://github.com/3rdPix/vscode-suassembly-syntax-highlighter/issues"
    },
    "categories": ["Programming Languages", "Language Packs"],
    "contributes":
    {
      "languages":
        [
          {
            "id": "suasm",
            "aliases": ["Su-Assembly", "Su-Asm"],
            "extensions": [".suasm"],
            "icon": {"light": "./images/suasm-light.svg", "dark": "./images/suasm-dark.svg"},
            "configuration": "./language-configuration.json"
          }
        ],
      "grammars":
        [
          {
            "language": "suasm",
            "scopeName": "source.suasm",
            "path": "./syntaxes/suasm.tmLanguage.json"
          }
        ],
      "snippets":
        [
          {
              "language": "suasm",
              "path": "./snippets/snippets.json"
          }
        ]
    }
  }
